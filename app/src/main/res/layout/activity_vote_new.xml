                        style="@style/style_vote_name"
                        android:text="方某某">

                    </com.ydd.conference.custom.HuaWenTextView>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout12"
                    style="@style/style_vote_layout">

                    <com.ydd.conference.custom.HuaWenTextView
                        android:id="@+id/name12Text"
                        style="@style/style_vote_name"
                        android:text="方某某">

                    </com.ydd.conference.custom.HuaWenTextView>
                </FrameLayout>

            </LinearLayout>

            <LinearLayout style="@style/style_vote_linear">

                <FrameLayout
                    android:id="@+id/frameLayout13"
                    style="@style/style_vote_layout">

                    <com.ydd.conference.custom.HuaWenTextView
                        android:id="@+id/name13Text"
                        style="@style/style_vote_name"
                        android:text="方某某">

                    </com.ydd.conference.custom.HuaWenTextView>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout14"
                    style="@style/style_vote_layout">

                    <com.ydd.conference.custom.HuaWenTextView
                        android:id="@+id/name14Text"
                        style="@style/style_vote_name"
                        android:text="方某某">

                    </com.ydd.conference.custom.HuaWenTextView>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout15"
                    style="@style/style_vote_layout">

                    <com.ydd.conference.custom.HuaWenTextView
                        android:id="@+id/name15Text"
                        style="@style/style_vote_name"
                        android:text="方某某">

                    </com.ydd.conference.custom.HuaWenTextView>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout16"
                    style="@style/style_vote_layout">

                    <com.ydd.conference.custom.HuaWenTextView
                        android:id="@+id/name16Text"
                        style="@style/style_vote_name"
                        android:text="方某某">

                    </com.ydd.conference.custom.HuaWenTextView>
                </FrameLayout>

            </LinearLayout>

        </com.ydd.conference.custom.InterceptLinearLayout>
    </LinearLayout>


    <LinearLayout
        android:id="@+id/submitLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="@color/blue"
        android:gravity="center"
        android:orientation="vertical"
        android:visibility="gone">

        <com.ydd.conference.custom.HuaWenTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="是否提交?"
            android:textColor="@color/white"
            android:textSize="100sp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="140dp"
            android:orientation="horizontal">

            <com.ydd.conference.custom.HuaWenTextView
                android:id="@+id/submitText"
                style="@style/style_submit_text"
                android:background="@mipmap/btn_submit_bg"
                android:text="提交" />

            <com.ydd.conference.custom.HuaWenTextView
                android:id="@+id/cancelText"
                style="@style/style_submit_text"
                android:layout_marginLeft="150dp"
                android:text="取消" />

        </LinearLayout>

    </LinearLayout>

</FrameLayout>